<h1>Listing user_meta</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_meta.each do |user_metum| %>
      <tr>
        <td><%= link_to 'Show', user_metum %></td>
        <td><%= link_to 'Edit', edit_user_metum_path(user_metum) %></td>
        <td><%= link_to 'Destroy', user_metum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User metum', new_user_metum_path %>
